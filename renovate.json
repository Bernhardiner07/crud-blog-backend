{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:linters",
    "group:linters",
    "group:recommended",
    ":semanticCommits",
    ":semanticCommitTypeAll(deps)",
    ":semanticCommitScope(renovate)"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "groupName": "dependencies"
    }
  ]
}

